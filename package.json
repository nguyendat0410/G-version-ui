  },
  "homepage": "https://nguyendat0410.github.io/getfly-ui/",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "packageManager": "yarn@3.6.3",
